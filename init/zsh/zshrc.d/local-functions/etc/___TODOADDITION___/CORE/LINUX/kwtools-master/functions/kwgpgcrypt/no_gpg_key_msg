# Funktion no_gpg_key_msg{{{
#
# usage: no_gpg_key_msg
no_gpg_key_msg() {
	MSG=$(gettext 'Es sind keine Schluessel vorhanden.')
	msgbox "$gv_Attention" "$MSG"
}
#}}}
