# Funktion, keine RAID Partitionen vorhanden# {{{
#
# usage: no_raid_part_msg
no_raid_part_msg() {
	MSG=$(gettext 'Es sind keine Partitionen, vom Typ raid/fd (RAID), vorhanden.')
	msgbox "$gv_Attention" "$MSG"
}
# }}}
