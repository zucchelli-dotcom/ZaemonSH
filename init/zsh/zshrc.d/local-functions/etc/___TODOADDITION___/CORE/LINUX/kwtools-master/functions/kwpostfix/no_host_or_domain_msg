# Funktion, kein Rechner oder Domain angegeben# {{{
# usage: no_host_or_domain_msg
no_host_or_domain_msg() {
	MSG=$(gettext 'Sie muessen erst einen Rechnernamen oder einen Domainnamen eingeben.')
	msgbox "$gv_Attention" "$MSG"
}
## }}}
