# Funktion no_luks_cryptdev_msg# {{{
no_luks_cryptdev_msg() {
	MSG=$(gettext 'Es ist kein verschluesseltes Laufwerk, mit LUKS Erweiterung, vorhanden.')
	msgbox "$gv_Info" "$MSG"
}
## }}}
