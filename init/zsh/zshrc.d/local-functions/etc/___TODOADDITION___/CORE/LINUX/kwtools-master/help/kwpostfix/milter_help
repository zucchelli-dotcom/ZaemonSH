milter_connect_macros: Standard ist "j {daemon_name} v".

milter_connect_timeout: Standard ist "30s".

milter_content_timeout: Standard ist "300s".

milter_data_macros: Standard ist "i".

milter_default_action: Standard ist "tempfail".

milter_end_of_data_macros: Standard ist "i".

milter_end_of_header_macros: Standard ist "i".

milter_header_checks:

milter_helo_macros: Standard ist "{tls_version} {cipher}
\t{cipher_bits} {cert_subject} {cert_issuer}".

milter_macro_daemon_name: Standard ist "$myhostname".

milter_macro_v: Standard ist "$mail_name $mail_version".

milter_mail_macros: Standard ist "i {auth_type} {auth_authen}
\t{auth_author} {mail_addr} {mail_host} {mail_mailer}".

milter_protocol: Standard ist "6".

milter_rcpt_macros: Standard ist "i {rcpt_addr} {rcpt_host}
\t{rcpt_mailer}".

milter_unknown_command_macros:
')
