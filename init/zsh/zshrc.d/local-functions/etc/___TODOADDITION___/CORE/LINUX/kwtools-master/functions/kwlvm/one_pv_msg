# Funktion one_pv_msg{{{
#
one_pv_msg() {
	MSG=$(gettext 'In dieser VG sind zu wenige PVs vorhanden um eine zu entfernen.')
	msgbox "$gv_Attention" "$MSG"
}
#}}}
