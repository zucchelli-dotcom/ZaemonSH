# Funktion no_crypt_sw_msg# {{{
#
# usage: no_crypt_sw_msg
#
no_crypt_sw_msg() {
	MSG=$(gettext 'Es ist keine Verschluessellungs-Software installiert.')
	msgbox "$gv_Attention" "$MSG"
}
## }}}
