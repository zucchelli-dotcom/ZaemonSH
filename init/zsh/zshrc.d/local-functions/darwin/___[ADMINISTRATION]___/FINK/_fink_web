#----------------------------------------------------------------------------------------#
#                                       CONFIG
#----------------------------------------------------------------------------------------#
all_avail_finkpkgs=($(cat ~/.zsh/cache/uninst_finkpkgs \
                         ~/.zsh/cache/inst_finkpkgs  2>/dev/null \
                         | command sort -u ))    
                         
#----------------------------------------------------------------------------------------#
#                                      FUNCTIONS
#----------------------------------------------------------------------------------------#

#----------------------------------------------------------------------------------------#
#                                      COMPLETION
#----------------------------------------------------------------------------------------#





                                         


_wanted packages expl 'packages' compadd "$all_avail_finkpkgs[@]"
