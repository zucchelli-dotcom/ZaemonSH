banner
killrunning_apps

if [[ ! -f $LOGFILE  ]]; then
    sudo touch /private/var/log/OSXDOCTOR.log
    sudo chown ${USER}:admin /private/var/log/OSXDOCTOR.log
    wait
    sleep 2
    wait
    #timestamp
    wait
    main
else
    sudo -l
    wait
    sleep 2
    wait
    #timestamp
    wait
    main
fi
