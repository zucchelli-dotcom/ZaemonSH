  #       subcommand2)
  #         _arguments \
  #           '-bar[subcommand2 -bar argument comment]' \
  #           '-baz[subcommand2 -baz argument comment]'
  #           '*:devices:_devices'
  #         ;;
  #       subcommand3)
  #         _arguments \
  #         ;;
  #     esac
  #     ;;
  # esac
